<h1>List of Stories</h1>
<div class="row">
  <% @stories.each do |story| %>
    <div class="col-md-3 col-sm-4">
      <div class="thumbnail">
        <%= link_to image_tag("http://via.placeholder.com/125x200"), story %>
        <div class="caption text-center">
          <%= link_to story.title, story %>
        </div>
      </div>
    </div>
  <% end %>
</div>
